{
  "name": "Fight now",
  "version": "1.0",
  "manifest_version": 3,
  "default_locale": "en",
  "description": "__MSG_description__",
  "permissions": [
    "storage",
    "notifications",
    "alarms"
  ],
  "action": {
      "default_icon": {
          "16": "/icons/16.png",
          "24": "/icons/24.png",
          "32": "/icons/32.png",
          "48": "/icons/48.png",
          "64": "/icons/64.png",
          "128": "/icons/128.png"
      },
      "default_popup": "popup.html",
      "default_title": "Fight now"
  },
  "icons": {
    "16": "/icons/16.png",
    "24": "/icons/24.png",
    "32": "/icons/32.png",
    "48": "/icons/48.png",
    "64": "/icons/64.png",
    "128": "/icons/128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}